<Window x:Class="SF11_2019_POP2020.Windows.DodavanjaIzmenaDoktora"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SF11_2019_POP2020.Windows"
        mc:Ignorable="d"
        Title="DodavanjaIzmenaDoktora" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>

        </Grid.RowDefinitions>

        <Label Grid.Column="0" Name="lblDomZdravljaId" Content="DomZdravlja:"/>
        <ComboBox Name="cmbDomoviZdravlja" Margin="10" Grid.Column="1" SelectionChanged="cmbDomoviZdravlja_SelectionChanged"/>



        <Button Grid.Column="0" Grid.Row="3" Content="Cancel" Name="btnCancel" Click="btnCancel_Click"></Button>
        <Button Grid.Column="1" Grid.Row="3" Content="Ok" Name="btnOk" Click="btnOk_Click"></Button>

    </Grid>
</Window>
