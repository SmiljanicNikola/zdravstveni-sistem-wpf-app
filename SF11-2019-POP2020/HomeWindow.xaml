        <Button Grid.ColumnSpan="3" Content="Nastavi" HorizontalAlignment="Left" Margin="663,376,0,0" VerticalAlignment="Top" Width="75"/>
        <Label Grid.ColumnSpan="3" Content="Nastavite bez naloga?" HorizontalAlignment="Left" Margin="635,345,0,0" VerticalAlignment="Top"/>
        <GroupBox Grid.ColumnSpan="3" Header="Ulogujte se kao: " HorizontalAlignment="Left" Height="90" Margin="245,83,0,0" VerticalAlignment="Top" Width="334"/>
        <GroupBox Grid.ColumnSpan="3" Header="Registrujte se" HorizontalAlignment="Left" Height="111" Margin="324,187,0,0" VerticalAlignment="Top" Width="162"/>


    </Grid>
